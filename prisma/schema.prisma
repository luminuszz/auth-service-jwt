datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}


model User {
    id      String @id @default(uuid())
    name    String 
    email    String @unique
    phone   String?
    password String
    updated_at DateTime @updatedAt
    created_at DateTime @default(now())

    @@map(name: "users")
}